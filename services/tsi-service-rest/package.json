    "@types/swagger-ui-express": "^4.1.2",
    "async-redis": "^1.1.7",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "bull": "^3.19.1",
    "chalk": "^4.1.0",
    "cloudinary": "^1.23.0",
    "compression": "^1.7.4",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "google-auth-library": "^6.1.3",
    "inversify": "^5.0.1",
    "iterare": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.get": "^4.4.2",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "node-notifier": "^8.0.0",
    "qs": "^6.9.4",
    "redis": "^3.0.2",
