{
  "name": "tsi-service-data",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts'",
    "build": "rimraf ./dist && tsc -p .",
    "checkFiles": "tsc --listFiles",
    "start": "ts-node dist/src/index.js",
    "db-migrate": "npm run build && sequelize db:migrate $* --options-path sequelizerc/migrate.sequelizerc",
    "db-migrate:undo": "npm run build && sequelize db:migrate:undo:all $* --options-path sequelizerc/migrate.sequelizerc",
    "db-seed": "npm run build && sequelize db:seed:all $* --options-path sequelizerc/migrate.sequelizerc",
    "db-seed:undo": "npm run build && sequelize db:seed:undo:all $* --options-path sequelizerc/migrate.sequelizerc",
    "prettier": "prettier --write src/**/*.{ts,js}",
    "lint:ts": "eslint --quiet --color --no-error-on-unmatched-pattern src --ext .ts,.js",
    "lint:ts:fix": "eslint --quiet --color --no-error-on-unmatched-pattern --fix src --ext .ts,.js",
    "precommit": "lint-staged"
  },
  "author": "Togushakov Oleg",
  "license": "ISC",
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "npm run prettier",
      "npm run lint:ts"
    ]
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.8",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^14.11.2",
    "@types/node-notifier": "^8.0.0",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "city-timezones": "^1.2.0",
    "cron": "^1.8.2",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "iterare": "^1.2.1",
    "lodash.get": "^4.4.2",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "mysql2": "^2.2.5",
    "node-notifier": "^8.0.0",
    "node-worker-threads-pool": "^1.4.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.2",
    "tunnel-ssh": "^4.1.4",
    "typescript": "^4.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "lint-staged": "^10.4.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2"
  },
  "_moduleAliases": {
    "abstractions": "src/abstractions.ts",
    "db": "src/db",
    "DIContainer": "src/DIContainer",
    "config": "src/config",
    "constants": "src/constants",
    "modules": "src/modules",
    "services": "src/services",
    "utils": "src/utils"
  }
}
